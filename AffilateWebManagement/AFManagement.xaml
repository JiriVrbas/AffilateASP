<Window x:Class="AffilateWebManagement.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AffilateWebManagement"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="Auto"/>
            <ColumnDefinition Width="*"/>
        </Grid.ColumnDefinitions>
        <Grid Grid.Column="0">
            <Grid.RowDefinitions>
                <RowDefinition Height="*"/>
                <RowDefinition Height="*"/>
                <RowDefinition Height="*"/>
                <RowDefinition Height="*"/>
                <RowDefinition Height="*"/>
            </Grid.RowDefinitions>
            <Button Grid.Row="0" Content="Správa uživatelů"/>
            <Button Grid.Row="1" Content="Správa affilate partnerů"/>
            <Button Grid.Row="2" Content="Správa provizí"/>
            <Button Grid.Row="3" Content="Rozdělení provize uživateli"/>
            <Button Grid.Row="4" Content="Správa linků"/>
            <Button Grid.Row="5" Content="xxxxxx"/>
        </Grid>
    </Grid>
</Window>
